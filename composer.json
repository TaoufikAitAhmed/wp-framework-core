{
  "name": "themes/wp-framework-core",
  "authors": [
    {
      "name": "Taoufik",
      "email": "taoufikaitahmed21@gmail.com"
    }
  ],
  "repositories": {
    "0": {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com/"
    }
  },
  "require": {
    "php": "^8.2",
    "rareloop/lumberjack-core": "^6.0",
    "stoutlogic/acf-builder": "^1.12",
    "illuminate/support": "^8.83",
    "symfony/finder": "^5.4",
    "symfony/console": "^5.4",
    "icanboogie/inflector": "^2.2",
    "ramsey/uuid": "^4.7",
    "ext-json": "*",
    "djboris88/timber-debugger": "^1.1",
    "illuminate/filesystem": "^8.79",
    "symfony/filesystem": "^5.4",
    "league/flysystem": "1.x",
    "illuminate/console": "^8.80",
    "symfony/yaml": "^5.4",
    "twig/twig": "^2.16"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "phpmd/phpmd": "dev-master",
    "slevomat/coding-standard": "^7.0",
    "szepeviktor/phpstan-wordpress": "^1.0.0",
    "paulthewalton/acf-stubs": "^5.8",
    "paulthewalton/acf-pro-stubs": "5.8.9.1",
    "php-mock/php-mock": "^2.3",
    "codedungeon/phpunit-result-printer": "^0.32.0",
    "mikey179/vfsstream": "^1.6",
    "wp-phpunit/wp-phpunit": "6.0.0",
    "roots/wordpress": "^5.8",
    "brain/monkey": "^2.6",
    "yoast/phpunit-polyfills": "^1.0",
    "wpengine/advanced-custom-fields-pro": "*",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.19",
    "themes/php-cs-fixer-config-themes": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "themes\\Wordpress\\Framework\\Core\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "themes\\Wordpress\\Framework\\Core\\Test\\": "tests",
      "Database\\": "tests/theme/database",
      "Database\\Seeders\\": "tests/theme/database/seeders"
    }
  },
  "extra": {
    "installer-paths": {
      "tests/wordpress/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin",
        "type:wordpress-muplugin"
      ],
      "tests/wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "tests/wordpress"
  },
  "scripts": {
    "lint": [
      "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "phpstan": [
      "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit 1G"
    ],
    "lint:fix": [
      "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ],
    "integration-test": [
      "cd tests/Integration && ../../vendor/bin/phpunit --testdox"
    ],
    "unit-test": [
      "cd tests/Unit && ../../vendor/bin/phpunit --testdox"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "roots/wordpress-core-installer": true
    }
  }
}
